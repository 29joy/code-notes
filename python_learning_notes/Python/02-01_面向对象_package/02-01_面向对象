"""
# __str__
class Student:

    def __init__(self, name, age, address):
        self.name = name
        self.age = age
        self.address = address
stu = Student("zhangsan", 22, "beijing")
print(stu)# è¾“å‡ºä¸º<__main__.Student object at 0x00000260A5585A90>
print(str(stu))# è¾“å‡ºåŒä¸Š, éƒ½æ˜¯è¾“å‡ºstuåœ¨å†…å­˜ä¸­çš„å­˜å‚¨åœ°å€
# ä½†æ˜¯å¯¹è±¡çš„å­˜å‚¨åœ°å€ä¸€èˆ¬ä½œç”¨ä¸å¤§, é€šè¿‡__str__æ–¹æ³•, æ§åˆ¶ç±»è½¬æ¢ä¸ºå­—ç¬¦ä¸²çš„è¡Œä¸º
class Student:

    def __init__(self, name, age):
        self.name = name
        self.age = age

    def __str__(self):
        return f"Studentç±»å¯¹è±¡, name={self.name},  age={self.age}"

stu = Student("zhangsan", 22)
print(stu)# è¾“å‡ºä¸ºStudentç±»å¯¹è±¡, name=zhangsan,  age=22
print(str(stu))# è¾“å‡ºä¸ºStudentç±»å¯¹è±¡, name=zhangsan,  age=22


# __lt__
class Student:

    def __init__(self, name, age, address):
        self.name = name
        self.age = age

stu1 = Student("liuliu", 22)
stu2 = Student("bangbang", 20)
print(stu1 > stu2)
# æŠ¥é”™TypeError: '>' not supported between instances of 'Student' and 'Student'

class Student:

    def __init__(self, name, age):
        self.name = name
        self.age = age

    def __lt__(self, other):# __lt__æ–¹æ³•è‡ªå¸¦, è¡¨ç¤ºå¦ä¸€ä¸ªå¯¹è±¡
        return self.age < other.age# å®šä¹‰è¦æ¯”è¾ƒä¸¤ä¸ªå¯¹è±¡çš„age

stu1 = Student("liuliu", 22)
stu2 = Student("bangbang", 20)
print(stu1 > stu2)# è¾“å‡ºTrue
print(stu1 < stu2)# è¾“å‡ºFalse


# __le__
class Student:

    def __init__(self, name, age):
        self.name = name
        self.age = age

    def __lt__(self, other):# __lt__æ–¹æ³•è‡ªå¸¦, è¡¨ç¤ºå¦ä¸€ä¸ªå¯¹è±¡
        return self.age < other.age# å®šä¹‰è¦æ¯”è¾ƒä¸¤ä¸ªå¯¹è±¡çš„age

stu1 = Student("liuliu", 22)
stu2 = Student("bangbang", 20)
print(stu1 >= stu2)
print(stu1 <= stu2)
# æŠ¥é”™TypeError: '>=' not supported between instances of 'Student' and 'Student'
# è¿™æ˜¯å› ä¸º, å°½ç®¡å®šä¹‰äº†__lt__æ–¹æ³•, ä½†æ˜¯ç±»ä»ç„¶ä¸æ”¯æŒ<=å’Œ>=
class Student:

    def __init__(self, name, age):
        self.name = name
        self.age = age

    def __le__(self, other):
        return self.age <= other.age

stu1 = Student("liuliu", 22)
stu2 = Student("bangbang", 20)
stu3 = Student("bingbing", 22)
print(stu1 >= stu2)# è¾“å‡ºä¸ºTrue
print(stu1 <= stu2)# è¾“å‡ºä¸ºFalse
print(stu1 >= stu3)# è¾“å‡ºä¸ºTrue


# __eq__
class Student:

    def __init__(self, name, age):
        self.name = name
        self.age = age

stu1 = Student("liuliu", 22)
stu2 = Student("bangbang", 20)
stu3 = Student("bingbing", 22)
print(stu1 == stu3)# è¾“å‡ºä¸ºFalse=====================>ä¸å®šä¹‰__eq__æ–¹æ³•==é»˜è®¤æ¯”è¾ƒå†…å­˜åœ°å€, é‚£ä¹ˆä¸¤ä¸ªå¯¹è±¡çš„å†…å­˜åœ°å€å½“ç„¶ä¸ç›¸ç­‰

class Student:

    def __init__(self, name, age):
        self.name = name
        self.age = age

    def __eq__(self, other):
        return self.age == other.age

stu1 = Student("liuliu", 22)
stu2 = Student("bangbang", 20)
stu3 = Student("bingbing", 22)
print(stu1 == stu2)# è¾“å‡ºä¸ºFalse
print(stu1 == stu3)# å®ç°__eq__æ–¹æ³•ä¹‹åè¾“å‡ºä¸ºTrue


