print(stu1.name)
print(stu1.gender)
print(stu1.age)
# ç»ƒä¹ 
class Student:

    def __init__(self, name, age, address):
        self.name = name
        self.age = age
        self.address = address

num = input("è¯·è¾“å…¥æœ¬æ¬¡éœ€è¦å½•å…¥ä¿¡æ¯çš„å­¦ç”Ÿæ•°é‡:")
for i in range(int(num)):
    print(f"å½“å‰å½•å…¥ç¬¬{i + 1}ä½å­¦ç”Ÿä¿¡æ¯, æ€»å…±éœ€è¦å½•å…¥{num}ä½å­¦ç”Ÿä¿¡æ¯")
    name = input("è¯·è¾“å…¥å­¦ç”Ÿå§“å:")
    age = input("è¯·è¾“å…¥å­¦ç”Ÿå¹´é¾„:")
    address = input("è¯·è¾“å…¥å­¦ç”Ÿåœ°å€:")
    stu = Student(name, age, address)
    print(f"å­¦ç”Ÿ{i + 1}ä¿¡æ¯å½•å…¥å®Œæˆ, ä¿¡æ¯ä¸º: ã€å­¦ç”Ÿå§“å: {stu.name}, å¹´é¾„: {stu.age}, åœ°å€: {stu.address}")
print("æ‰€æœ‰å­¦ç”Ÿä¿¡æ¯å½•å…¥å®Œæ¯•")


